@model Yildiz.Models.Cikis
@{
    ViewData["Title"] = "Çıkış Detay";
}
<h2>Çıkış Detay</h2>
<dl class="row">
    <dt class="col-sm-2">Sanat Eseri</dt>
    <dd class="col-sm-10">@Model.SanatEseri?.Baslik</dd>
    <dt class="col-sm-2">Çıkış Tarihi</dt>
    <dd class="col-sm-10">@Model.CikisTarihi.ToShortDateString()</dd>
</dl>
<a class="btn btn-secondary" asp-action="Listele">Geri Dön</a> 