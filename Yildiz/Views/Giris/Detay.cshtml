@model Yildiz.Models.Giris
@{
    ViewData["Title"] = "Giriş Detay";
}
<h2>Giriş Detay</h2>
<dl class="row">
    <dt class="col-sm-2">Sanat Eseri</dt>
    <dd class="col-sm-10">@Model.SanatEseri?.Baslik</dd>
    <dt class="col-sm-2">Giriş Tarihi</dt>
    <dd class="col-sm-10">@Model.GirisTarihi.ToShortDateString()</dd>
</dl>
<a class="btn btn-secondary" asp-action="Listele">Geri Dön</a> 